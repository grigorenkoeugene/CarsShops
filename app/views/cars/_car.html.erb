
<div id="<%= dom_id car %>">
  <p>
    <strong>Make:</strong>
    <%= car.make %>
  </p>

  <p>
    <strong>Model:</strong>
    <%= car.model %>
  </p>

  <p>
    <strong>Year:</strong>
    <%= car.year %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= car.price %>
  </p>

  <p>
    <strong>Odometer:</strong>
    <%= car.odometer %>
  </p>

  <p>
    <strong>Shop:</strong>
    <%= car.shop.name %>
  </p>

  <p>
    <strong>User:</strong>
    <%= car.user_id %>
  </p>

</div>
